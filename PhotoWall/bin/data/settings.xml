<options>
	<powersave>0</powersave>
	<debug>1</debug>
    <sendCommands>0</sendCommands>
</options>

<path>
	<appName>PhotoWall</appName>
	<appDirectory>~/Desktop/openFrameworks/apps/DohaInstallation/PhotoWall/bin</appDirectory>
</path>

<defaults>
	<screen width="1920" height="1080" bevel="100"/>
</defaults>

<computers>
	<computer hostname="3rdi1">
		<card>
			<screen x="0" y="0"/>
		</card>
		<card>
			<screen x="1" y="0"/>
		</card>
	</computer>
	<computer hostname="3rdi2">
		<card>
			<screen x="2" y="0"/>
		</card>
		<card>
			<screen x="0" y="1"/>
		</card>
	</computer>
	<computer hostname="3rdi3">
		<card>
			<screen x="1" y="1"/>
		</card>
		<card>
			<screen x="2" y="1"/>
		</card>
	</computer>
	<computer hostname="3rdi4">
		<card>
			<screen x="1" y="2"/>
		</card>
